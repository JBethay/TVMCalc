<?xml version="1.0" encoding="utf-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="true" >
    <shape>
      <solid
          android:color="#FFD3D3D3" />
      <stroke
          android:width="1dp"
          android:color="#FF778899" />
      <corners
          android:radius="6dp" />
      <padding
          android:left="5dp"
          android:top="5dp"
          android:right="5dp"
          android:bottom="5dp" />
    </shape>
  </item>
  <item>
    <shape>
      <gradient
          android:startColor="#808080"
          android:endColor="#FFD3D3D3"
          android:angle="270" />
      <stroke
          android:width="1dp"
          android:color="#FFD3D3D3" />
      <corners
          android:radius="6dp" />
      <padding
          android:left="5dp"
          android:top="5dp"
          android:right="5dp"
          android:bottom="5dp" />
    </shape>
  </item>
</selector>
